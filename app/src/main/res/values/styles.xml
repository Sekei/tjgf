<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="windowNoTitle">true</item>//无标题
        <item name="windowActionBar">false</item>//无ActionBar
    </style>
    <!--自定义底部tab-->
    <style name="RadioGroupButtonStyle">
        <!-- 这个属性是去掉button 默认样式-->
        <item name="android:button">@null</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">11sp</item>
        <item name="android:textColor">@color/home_text_color_selector</item>
    </style>

    <style name="MapRadioGroupStyle">
        <!-- 这个属性是去掉button 默认样式-->
        <item name="android:button">@null</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/colorWhite</item>
    </style>

    <!--自定义五角星评论-->
    <style name="RadingStyle" parent="@android:style/Widget.RatingBar">
        <!-- 定义星星图片 -->
        <item name="android:progressDrawable">@drawable/layer_live_rating_bar</item>
        <item name="android:minHeight">25dp</item>
        <item name="android:maxHeight">25dp</item>
    </style>

